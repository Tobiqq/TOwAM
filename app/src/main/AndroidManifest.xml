<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:andoid="http://schemas.android.com/tools"
    package="com.example.basketballmanagerv3">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddTeamActivity"></activity>
        <activity android:name=".AddGameActivity"></activity>
        <activity android:name=".GetPlayersActivity"></activity>
        <activity android:name=".AddPlayerActivity"></activity>
        <activity android:name=".GamesEndedActivity"></activity>
        <activity android:name=".TrackGameActivity"></activity>
        <activity android:name=".ChoseFirstFiveActivity"></activity>
        <activity android:name="ShowStats1Activity"></activity>
        <activity android:name="ShowStats2Activity"></activity>
        <activity android:name="GetPlayersStats"></activity>
    </application>

</manifest>